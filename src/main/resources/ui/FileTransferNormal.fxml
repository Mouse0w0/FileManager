<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Text?>
<fx:root prefHeight="64.0" prefWidth="338.0" type="BorderPane" xmlns="http://javafx.com/javafx/11.0.1"
         xmlns:fx="http://javafx.com/fxml/1">
    <center>
        <GridPane alignment="CENTER" hgap="5.0" BorderPane.alignment="CENTER">
            <columnConstraints>
                <ColumnConstraints halignment="RIGHT" hgrow="SOMETIMES" maxWidth="224.0" minWidth="0.0"
                                   prefWidth="8.0"/>
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="236.0" minWidth="10.0" prefWidth="228.0"/>
            </columnConstraints>
            <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            </rowConstraints>
            <children>
                <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Target Path:"/>
                <HBox alignment="CENTER" GridPane.columnIndex="1">
                    <children>
                        <TextField fx:id="path" prefHeight="23.0" prefWidth="357.0"/>
                        <Button mnemonicParsing="false" onAction="#openDirectoryChooser" prefHeight="23.0"
                                prefWidth="48.0" text="..."/>
                    </children>
                </HBox>
            </children>
        </GridPane>
    </center>
    <bottom>
        <HBox alignment="CENTER_RIGHT" BorderPane.alignment="CENTER">
            <children>
                <Button mnemonicParsing="false" onAction="#save" text="Save"/>
            </children>
        </HBox>
    </bottom>
    <padding>
        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
    </padding>
</fx:root>
